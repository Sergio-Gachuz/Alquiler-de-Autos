        <TextField fx:id="txtTelefono" layoutX="348.0" layoutY="135.0" prefHeight="31.0" prefWidth="270.0" promptText="Telefono" />
        <Button fx:id="btnGuardar" layoutX="99.0" layoutY="330.0" mnemonicParsing="false" text="Guardar">
            <font>
                <Font size="21.0" />
            </font>
        </Button>
        <Button fx:id="btnCancelar" layoutX="475.0" layoutY="330.0" mnemonicParsing="false" text="Cancelar">
            <font>
                <Font size="21.0" />
            </font>
        </Button>
      <TextField layoutX="348.0" layoutY="233.0" prefHeight="31.0" prefWidth="270.0" promptText="Estado" fx:id="txtEstado"/>
    </children>
</AnchorPane>
