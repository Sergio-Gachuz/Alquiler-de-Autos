<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fitHeight="464.0" fitWidth="695.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Resources/fondo.jpg" />
            </image>
        </ImageView>
        <Label layoutX="14.0" layoutY="14.0" text="Informacion de Vehiculo" textFill="WHITE">
            <font>
                <Font size="26.0" />
            </font>
        </Label>
        <Label layoutX="15.0" layoutY="96.0" text="Clave Agencia:" textFill="WHITE">
            <font>
                <Font size="21.0" />
            </font>
        </Label>
        <Label layoutX="18.0" layoutY="202.0" text="Direccion:" textFill="WHITE">
            <font>
                <Font size="21.0" />
            </font>
        </Label>
        <Label layoutX="348.0" layoutY="96.0" text="Telefono:" textFill="WHITE">
            <font>
                <Font size="21.0" />
            </font>
        </Label>
        <Label layoutX="348.0" layoutY="202.0" text="Estado:" textFill="WHITE">
            <font>
                <Font size="21.0" />
            </font>
        </Label>
        <TextField fx:id="txtID" layoutX="18.0" layoutY="135.0" prefHeight="31.0" prefWidth="270.0" promptText="Clave" />
